<div id="DigitoCPF_${instanceId}" class="super-widget wcm-widget-class fluig-style-guide" data-params="DigitoCPF.instance()">
	<h2>Digite um CPF qualquer e clique em Gerar</h2>

	<div class="row">
		<div class="col-sm-4">
			<label for="cpf">CPF</label>
			<input type="text" id="cpf" class="form-control" maxlength="9">
		</div>
		<div class="col-sm-2">
			<br>
			<button class="btn btn-primary" data-gerar-digitos>Gerar</button>
		</div>
	</div>

	<div class="row">
		<div class="col-sm-4">
			<label for="cpfCompleto">CPF Gerado</label>
			<input type="text" id="cpfCompleto" class="form-control" readonly>
		</div>
	</div>
</div>

